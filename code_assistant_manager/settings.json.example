{
  "common": {
    "http_proxy": "http://proxy.example.com:8080/",
    "https_proxy": "http://proxy.example.com:8080/",
    "no_proxy": "localhost,127.0.0.1,192.168.1.100",
    "cache_ttl_seconds": 86400
  },
  "endpoints": {
    "litellm": {
      "endpoint": "https://192.168.1.100:4142",
      "api_key_env": "API_KEY_LITELLM",
      "list_models_cmd": "curl -s -X 'GET' 'https://192.168.1.100:4142/v1/models?return_wildcard_routes=false&include_model_access_groups=false&only_model_access_groups=false&include_metadata=false' -H 'accept: application/json' -H \"x-litellm-api-key: ${API_KEY_LITELLM}\"|jq -r '.data.[].id'",
      "keep_proxy_config": false,
      "use_proxy": false,
      "description": "Litellm support OpenAI compatible API calls",
      "supported_client": "claude,copilot,droid,codebuddy,codex,qwen"
    },
    "copilot-api": {
      "endpoint": "https://192.168.1.100:5000",
      "api_key_env": "API_KEY_COPILOT",
      "list_models_cmd": "python -m code_assistant_manager.copilot_models",
      "keep_proxy_config": true,
      "use_proxy": true,
      "supported_client": "claude,copilot,droid,codebuddy,codex,qwen,neovate",
      "description": "Github Copilot API Proxy"
    },
    "openai-compatible:dashscope-qwen3-max": {
      "endpoint": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "api_key_env": "API_KEY_QWEN",
      "list_models_cmd": "echo qwen3-max",
      "use_proxy": false,
      "description": "Alibaba Dashscope Endpoint",
      "supported_client": "qwen,droid,iflow"
    },
    "openai-compatible:dashscope-qwen3-coder-plus": {
      "endpoint": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "api_key_env": "API_KEY_QWEN",
      "list_models_cmd": "echo qwen3-coder-plus",
      "use_proxy": false,
      "description": "Alibaba Dashscope Endpoint",
      "supported_client": "qwen,droid,iflow"
    },
    "qwen-claude-proxy:dashscope-qwen3-coder-plus": {
      "endpoint": "https://dashscope.aliyuncs.com/api/v2/apps/claude-code-proxy",
      "api_key_env": "API_KEY_QWEN",
      "list_models_cmd": "echo qwen3-coder-plus",
      "use_proxy": false,
      "description": "Alibaba Dashscope Endpoint Claude Proxy",
      "supported_client": "claude"
    }
  }
}
